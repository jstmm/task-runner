{
  "version": "0.2.0",
  "configurations": [
  {
    "name": "DEBUG",
    "type": "cppdbg",
    "request": "launch",
    "program": "${workspaceFolder}/task_runner",
    "args": [
      "first_task",
      "second_task",
      "third_task",
    ],
    "stopAtEntry": false,
    "cwd": "${fileDirname}",
    "environment": [],
    "externalConsole": false,
    "MIMode": "gdb",
    "setupCommands": [
        {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
        },
        {
            "description": "Set Disassembly Flavor to Intel",
            "text": "-gdb-set disassembly-flavor intel",
            "ignoreFailures": true
        }
    ],
    "preLaunchTask": "BUILD"
  }
  ]
}
